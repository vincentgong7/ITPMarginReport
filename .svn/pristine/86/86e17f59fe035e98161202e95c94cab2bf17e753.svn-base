package itpreneurs.itp.report.parser;

/**
 * @author vincentgong
 *
 */
public class MyRow {
	public int originRowNumber;
	public String[] values;
	
	public MyRow(int originRowNumber, String[] values){
		this.originRowNumber = originRowNumber;
		this.values = values;
	}

	public String getVal(int colIndex) {
		// TODO Auto-generated method stub
		if(colIndex<=values.length){
			return values[colIndex];
		}
		return "";
	}
}
